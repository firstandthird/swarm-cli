#!/bin/bash

if [ "$1" == "_description" ]; then
  echo "[configFile] [name]"
  exit
fi

main() {
  local configFile=$1
  local name=$2
  if test -z "$name"; then
    name=$configFile
  fi
  . $DIR/swarm-copy $configFile
  log "creating $name ($configFile) config"
  . $DIR/swarm-shell "docker config create $name $configFile"
}
main "$@"
